  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "distribution": "store",
      "autoIncrement": false,
      "node": "18.18.0",
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "EXPO_USE_NPM": "1",
        "EXPO_NO_BUN": "1"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
