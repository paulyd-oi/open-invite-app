{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "distribution": "store",
      "autoIncrement": false,
      "node": "18.20.2",
      "npm": "9.8.1",
      "installCommand": "npm ci --include=dev",
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "EXPO_USE_NPM": "1",
        "EXPO_NO_BUN": "1"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}

