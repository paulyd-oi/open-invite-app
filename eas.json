{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.20.2",
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "EXPO_USE_NPM": "1",
        "EXPO_NO_BUN": "1",
        "NPM_CONFIG_INCLUDE": "dev"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": false,
      "node": "18.20.2",
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "EXPO_USE_NPM": "1",
        "EXPO_NO_BUN": "1",
        "NPM_CONFIG_INCLUDE": "dev"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
