{
  "cli": {
    "version": ">= 7.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.20.2",
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "EXPO_USE_NPM": "1",
        "EXPO_NO_BUN": "1",
        "NPM_CONFIG_INCLUDE": "dev",
        "EXPO_PUBLIC_API_URL": "https://open-invite-api.onrender.com"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": false
      }
    },

    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.20.2",
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "EXPO_USE_NPM": "1",
        "EXPO_NO_BUN": "1",
        "NPM_CONFIG_INCLUDE": "dev",
        "EXPO_PUBLIC_API_URL": "https://open-invite-api.onrender.com"
      },
      "ios": {
        "simulator": true
      }
    },

    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "node": "18.20.2",
      "env": {
        "NPM_CONFIG_PACKAGE_MANAGER": "npm",
        "EXPO_USE_NPM": "1",
        "EXPO_NO_BUN": "1",
        "NPM_CONFIG_INCLUDE": "dev",
        "EXPO_PUBLIC_API_URL": "https://open-invite-api.onrender.com"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
